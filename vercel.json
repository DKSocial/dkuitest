{
  "rewrites": [
    {
      "source": "/feed/posts/:id",
      "destination": "/feed/posts/[id].html"
    },
    {
      "source": "/user/:handle",
      "destination": "/user/[handle].html"
    },
    {
      "source": "/mobile-login/:sessionId",
      "destination": "/log/mobile-login/[sessionId].html"
    },
    {
      "source": "/feed/main",
      "destination": "/feed/feed.html"
    },
    {
      "source": "/feed/inbox",
      "destination": "/feed/chat.html"
    },
    {
      "source": "/feed/explore",
      "destination": "/feed/explore.html"
    },
    {
      "source": "/feed/settings",
      "destination": "/feed/settings.html"
    }
  ]
} 
